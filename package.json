    "axios": "^0.19.2",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^1.8.1",
    "nexmo-client": "^6.0.10",
    "react": "^16.13.1",
    "react-async": "^10.0.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0"
